<?xml version="1.0" encoding="UTF-8"?>
<completions>
    <!-- "foo" -->
    <provider name="perl.values">
        <syntax>perl</syntax>
        
        <exclude-selector>string, comment</exclude-selector>
        
        <expression>(?:(?&lt;!\.)\b([a-zA-Z_][a-zA-Z0-9_]*))|(?:(?&lt;![a-zA-Z0-9_\.]))</expression>
        
        <symbols type="function,method">
            <behavior>
                <arguments separator=" " prepend-separator="true" append-empty="false" />
            </behavior>
        </symbols>
        <symbols type="class" />
        <symbols type="variable,constant" />
        
        <identifiers />
    </provider>
</completions>
