<?xml version="1.0" encoding="UTF-8"?>
<!--
    These completions are up to date as of:
    mdn/data commit `af8a84a` on Jan 23, 2024.
-->
<completions>
    <provider name="pseudoelements">
        <syntax>css</syntax>
        
        <query>(pseudo_element_selector)</query>
        
        <expression>(?&lt;=::)[a-zA-Z0-9-]*</expression>
        
        <trigger>:</trigger>
        
        <set>css.pseudoelements</set>
    </provider>
    <provider name="pseudoclasses">
        <syntax>css</syntax>
        
        <query>(pseudo_class_selector)</query>
        
        <expression>(?&lt;!::)(?&lt;=:)[a-zA-Z0-9-]*</expression>
        
        <trigger>:</trigger>
        
        <set>css.pseudoclasses</set>
    </provider>
    
    <set name="css.pseudoclasses" symbol="style-pseudoclass">
